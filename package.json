{
  "name": "ta-core-lib",
  "version": "0.0.1",
  "private": true,
  "description": "Travelers App Core Lib",
  "author": "",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@FrancoAdN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrancoAdN/be-travelers-app.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint-md": "remark .",
    "format-md": "remark . -o",
    "format": "prettier --write \"lib/**/*.ts\" && npm run format-md",
    "lint": "eslint -c .eslintrc.js --ext ts lib/ && npm run lint-md",
    "lint:check": "eslint -c .eslintrc.js --ext .ts ./lib",
    "lint:ci:check": "eslint -c .eslintrc.js --ext .ts ./lib --quiet",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts ./lib --fix",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci:cov": "jest --runInBand --ci --reporters=jest-html-reporter --coverageReporters=lcov --coverage",
    "clean:dist": "rimraf dist",
    "clean:lock": "rimraf package-lock.json",
    "precise-commits": "precise-commits --check-only --whitelist=\"lib/**/*.ts\"",
    "husky": "npm run lint && npm run precise-commits"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky"
    }
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@types/passport-jwt": "^3.0.6",
    "aws-sdk": "^2.1096.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.13.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typescript": "4.4.4",
    "remark-cli": "^8.0.1",
    "precise-commits": "^1.0.2",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "ts-node": "^10.4.0",
    "husky": "^1.3.1",
    "lerna": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.3.2"
  }
}
